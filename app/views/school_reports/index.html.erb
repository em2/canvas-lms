<% jammit_css :reports_index -%>

<% content_for :page_title do -%>School Selection<% end -%>

<h1>View Reports for:</h1>

<table>
	<% @schools.each do |account| -%>
	  <tr>
	  	<td><%= link_to account.parent_account.name ? account.parent_account.name + account.name : account.name, school_report_assessment_reports_path(account.id), :class => "button"%> </td>
	  </tr>
  <% end -%>
</table>
